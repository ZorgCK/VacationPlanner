<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<?xdevuiml version="1.0"?>
<?template NONE?>
<?import com.vaadin.flow.component.button.Button?>
<?import com.vaadin.flow.component.html.Anchor?>
<?import com.vaadin.flow.component.html.Div?>
<?import com.vaadin.flow.component.html.Image?>
<?import com.vaadin.flow.component.html.Label?>
<?import com.vaadin.flow.component.orderedlayout.HorizontalLayout?>
<?import com.vaadin.flow.component.orderedlayout.VerticalLayout?>
<VerticalLayout xmlns:x="http://xml.xdev-software.com/xdevuiml" spacing="false" x:name="BeanTripItem" padding="false">
    <x:constraints zpos="0" width="100%" height="-1px" />
    <HorizontalLayout x:name="horizontalLayout">
        <x:constraints width="100%" height="100px" flex-grow="0.0" />
        <Image x:name="image" maxWidth="120px">
            <x:constraints width="100%" height="-1px" flex-grow="0.0" />
        </Image>
        <VerticalLayout x:name="verticalLayout2" padding="false">
            <x:constraints width="100%" height="100px" flex-grow="0.0" />
            <Label x:name="lblTitle" text="123">
                <x:constraints width="100%" height="0px" flex-grow="1.0" />
                <x:styles>
                    <entry name="white-space" value="pre-wrap" />
                    <entry name="word-wrap" value="break-word" />
                    <entry name="font-weight" value="bold" />
                    <entry name="font-size" value="18px" />
                </x:styles>
            </Label>
            <Div x:name="div3">
                <x:constraints width="-1px" height="-1px" flex-grow="0.0" />
            </Div>
        </VerticalLayout>
    </HorizontalLayout>
    <VerticalLayout spacing="false" x:name="verticalLayout" padding="false">
        <x:constraints width="100%" height="-1px" flex-grow="0.0" />
        <x:styles>
            <entry name="margin-top" value="5px" />
        </x:styles>
        <Anchor x:name="anchor" text="Anchor">
            <x:constraints width="0px" height="0px" flex-grow="0.0" />
        </Anchor>
        <Label x:name="lblDescription" text="Label">
            <x:constraints width="0px" height="0px" flex-grow="0.0" />
            <x:styles>
                <entry name="font-size" value="10px" />
            </x:styles>
        </Label>
    </VerticalLayout>
    <HorizontalLayout x:name="infoContainer" defaultVerticalComponentAlignment="com.vaadin.flow.component.orderedlayout.FlexComponent.Alignment.CENTER">
        <x:constraints width="100%" alignment="START" height="-1px" flex-grow="0.0" />
        <HorizontalLayout x:name="horizontalLayout2" defaultVerticalComponentAlignment="com.vaadin.flow.component.orderedlayout.FlexComponent.Alignment.CENTER">
            <x:constraints width="100px" alignment="CENTER" height="-1px" flex-grow="1.0" />
        </HorizontalLayout>
        <Button x:themeVariants="LUMO_SMALL LUMO_PRIMARY" x:name="button2">
            <x:icon src="com.vaadin.flow.component.icon.VaadinIcon#PENCIL" />
            <x:constraints width="0px" height="0px" flex-grow="0.0" />
            <x:event>click.onComponentEvent</x:event>
        </Button>
        <Button x:themeVariants="LUMO_SMALL LUMO_PRIMARY" x:name="button">
            <x:icon src="com.vaadin.flow.component.icon.VaadinIcon#TRASH" />
            <x:constraints width="0px" height="0px" flex-grow="0.0" />
            <x:event>click.onComponentEvent</x:event>
        </Button>
    </HorizontalLayout>
</VerticalLayout>
